{
    "compilerOptions": {
        "target": "ES2020",
        "module": "commonjs",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "outDir": "dist",
        "rootDir": "./",
        "skipLibCheck": true,
        "strict": true
    },
    "include": ["server/**/*"]
}
